<%= form_for :category, url: categories_path do |cat| %>
        <%= cat.label :name, :class => 'span8 input-large search-query' %>
        <%= cat.text_field :name, :class => 'span8 input-large'  %><br />
        <br />
        <%= cat.submit "Add Category", :class => 'btn btn-primary btn-sm' %>
      <% end %>